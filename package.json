{
  "name": "uns",
  "version": "0.1.0",
  "description": "Contracts and tools for UNS",
  "main": "index.js",
  "repository": "https://github.com/unstoppabledomains/uns.git",
  "private": true,
  "scripts": {
    "compile": "hardhat compile",
    "compile:size": "env ENABLE_CONTRACT_SIZER=true yarn compile",
    "coverage": "hardhat coverage",
    "test": "hardhat test",
    "lint": "prettier --list-different 'contracts/**/*.sol'",
    "deploy:localhost": "hardhat run --network localhost scripts/deploy.js",
    "deploy:rinkeby": "hardhat run --network rinkeby scripts/deploy.js",
    "deploy:proxy:localhost": "PROXY=true hardhat run --network localhost scripts/deploy.js",
    "upgrade:proxy:localhost": "hardhat run --network localhost scripts/upgrade.js",
    "start:node": "hardhat node",
    "gas-report": "env ENABLE_GAS_REPORT=true yarn test"
  },
  "devDependencies": {
    "@chainlink/contracts": "^0.1.7",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-solhint": "^2.0.0",
    "@nomiclabs/hardhat-truffle5": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts-upgradeable": "^4.1.0",
    "@openzeppelin/hardhat-upgrades": "^1.7.0",
    "chai": "^4.3.4",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.1.4",
    "hardhat": "^2.2.1",
    "hardhat-contract-sizer": "^2.0.3",
    "hardhat-gas-reporter": "^1.0.4",
    "node-fetch": "^2.6.1",
    "prettier": "^2.3.0",
    "prettier-plugin-solidity": "^1.0.0-beta.10",
    "solhint": "^3.3.4",
    "solidity-coverage": "^0.7.16",
    "web3": "^1.3.5",
    "yargs": "^16.2.0"
  }
}
